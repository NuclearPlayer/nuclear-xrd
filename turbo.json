{
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ],
      "env": [
        "TAURI_SIGNING_PRIVATE_KEY",
        "TAURI_SIGNING_PRIVATE_KEY_PASSWORD"
      ]
    },
    "@nuclearplayer/eslint-config#build": {
      "outputs": []
    },
    "@nuclearplayer/tailwind-config#build": {
      "outputs": []
    },
    "build:frontend": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:coverage": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^build"
      ]
    },
    "@nuclearplayer/storybook#dev": {
      "cache": false,
      "persistent": true
    },
    "@nuclearplayer/storybook#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "storybook-static/**"
      ]
    },
    "@nuclearplayer/website#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "out/**"
      ]
    }
  }
}