{
  "name": "@nuclearplayer/plugin-sdk",
  "version": "0.0.8",
  "description": "Plugin SDK for Nuclear music player",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:npm": "vite build --mode npm",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist build .turbo node_modules/.vite"
  },
  "dependencies": {
    "@nuclearplayer/ui": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.541.0",
    "react": "^18.3.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@nuclearplayer/eslint-config": "workspace:*",
    "@nuclearplayer/tailwind-config": "workspace:*",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.23",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "keywords": [
    "nuclear",
    "music",
    "player",
    "plugin",
    "sdk",
    "react"
  ],
  "author": {
    "name": "nukeop",
    "email": "12746779+nukeop@users.noreply.github.com"
  },
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/NuclearPlayer/nuclear-xrd.git",
    "directory": "packages/plugin-sdk"
  },
  "homepage": "https://github.com/NuclearPlayer/nuclear-xrd#readme",
  "bugs": {
    "url": "https://github.com/NuclearPlayer/nuclear-xrd/issues"
  }
}
